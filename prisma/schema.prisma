generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SolMeet {
  id          String   @id @default(uuid())
  username    String
  slug        String
  meetingId   Int
  title       String
  image       String   @db.Text
  description String?
  address     String
  price       Int
  createdAt   DateTime @default(now())
}
